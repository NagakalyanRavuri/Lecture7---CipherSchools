package corejava;

public class Switch {

	public static void main(String[] args) {
		int a = 1;
		switch(a) {
		case 1: System.out.println("run");
		break;
		case 2: System.out.println("walk");
		break;
		case 3: System.out.println("stop");
		break;
		default: System.out.println("Invalid number");
		}
	}

}
